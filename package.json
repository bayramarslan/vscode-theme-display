{
  "name": "theme-display",
  "displayName": "Theme Display",
  "description": "Displays the current theme name in the status bar",
  "icon": "images/icon.png",
  "publisher": "bayramarslan",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "theme",
    "statusbar",
    "color-scheme",
    "theme-name",
    "theme-display",
    "config",
    "shortcut"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "vsce package",
    "install": "powershell -ExecutionPolicy Bypass -File ./install.ps1",
    "vip": "npm run package && npm run install"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bayramarslan/vscode-theme-display.git"
  },
  "bugs": {
    "url": "https://github.com/bayramarslan/vscode-theme-display/issues"
  },
  "homepage": "https://github.com/bayramarslan/vscode-theme-display",
  "license": "MIT"
}
